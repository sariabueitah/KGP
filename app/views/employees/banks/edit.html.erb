<div class="mx-auto md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing Bank</h1>
  <%= render "form", bank: @bank, employee: :employee %>
  <%= link_to "Show this bank", employee_bank_path(@bank), class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  <%= link_to "Back to banks", employee_banks_path(@bank.employee_id), class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
</div>
